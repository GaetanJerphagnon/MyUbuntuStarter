// My Settings
{
    "workbench.sideBar.location": "right",
    "workbench.iconTheme": "vscode-great-icons",
    "explorer.confirmDelete": false,
    "emmet.includeLanguages": { "twig": "html" },
    "workbench.colorCustomizations": {

      // To check if theme is changing
      // "editor.findMatchBackground": "#e0cc44a8", //Current SEARCH MATCH
      // "editor.findMatchHighlightBackground": "#ff7b00a1" //Other SEARCH MATCHES

      "editor.selectionBackground": "#bfbfbf",
      "editor.selectionHighlightBorder": "#009dffbf",
      "editor.selectionHighlightBackground": "#009dff6f",
      "editor.background": "#e0e0e0",
      "editor.selection": "#f0f",
      // "editorCursor.foreground": "#ffffff",
      "terminalCursor.foreground": "#be8405",
      "inlineparameters.annotationForeground": "#a3a3a3",
      "inlineparameters.annotationBackground": "#ffffff10",
      "terminal.inactiveSelectionBackground": "#6c92996f"
    },
    "terminal.integrated.fontWeight": "bold",
    "editor.fontWeight": "bold",
    "settingsSync.ignoredExtensions": [


    ],
    "explorer.confirmDragAndDrop": false,
    "workbench.colorTheme": "Atom One Light",
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "diffEditor.codeLens": true,
    "colorize.languages": ["javascript", "scss", "css", "php"],
    "security.workspace.trust.untrustedFiles": "open",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.cursorWidth": 2,
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorWidth": 2,
    "terminal.integrated.fontFamily": "monospace",
    "terminal.integrated.minimumContrastRatio": 1,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "inline-parameters.borderRadius": "5px",
    "inline-parameters.fontStyle": "normal",
    "inline-parameters.margin": "2px",
    "inline-parameters.enabled": false,
    "todo-tree.general.tags": [
      "BUG",
      "HACK",
      "FIXME",
      "TODO",
      "XXX",
      "[ ]",
      "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.fontWeightBold": "normal",
    "editor.fontSize": 13,
    "workbench.preferredDarkColorTheme": "Atom One Dark",
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "scm.alwaysShowActions": true,
    "scm.alwaysShowRepositories": true,
    "files.trimTrailingWhitespace": true,
    "redhat.telemetry.enabled": false,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
      "*": false,
      "yaml": false,
      "plaintext": false,
      "markdown": false,
      "php": false,
      "smarty": false,
      "javascript": false
    },
    "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
    "php-cs-fixer.lastDownload": 1670425310590,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "none",
    "settingsSync.ignoredSettings": [

    ],
    "[json]": {
      "editor.defaultFormatter": "vscode.json-language-features"
    },
    "auto-close-tag.activationOnLanguage": [

      "xml",
      "php",
      "blade",
      "ejs",
      "jinja",
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "plaintext",
      "markdown",
      "vue",
      "liquid",
      "erb",
      "lang-cfml",
      "cfml",
      "HTML (EEx)",
      "HTML (Eex)",
      "plist"
    ],
    "files.autoSave": "onFocusChange",
    "workbench.startupEditor": "none",
    "terminal.integrated.enableMultiLinePasteWarning": false
  }
